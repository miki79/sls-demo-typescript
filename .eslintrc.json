{
  "env": {
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "plugin:jest/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "plugins": ["jest"],
  "settings": {
    "import/core-modules": ["aws-sdk"]
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": 0
  }
}
